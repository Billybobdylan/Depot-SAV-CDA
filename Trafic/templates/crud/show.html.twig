{% extends 'base.html.twig' %}

{% block title %}Module{% endblock %}

{% block body %}
    <h1>Module</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ module.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ module.nom }}</td>
            </tr>
            <tr>
                <th>Adresseip</th>
                <td>{{ module.adresseip }}</td>
            </tr>
            <tr>
                <th>Photo</th>
                <td>{{ module.photo }}</td>
            </tr>
            <tr>
                <th>Etat</th>
                <td>{{ module.etat ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>TypeModule</th>
                <td>{{ module.typeModule }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_crud_index') }}">back to list</a>

    <a href="{{ path('app_crud_edit', {'id': module.id}) }}">edit</a>

    {{ include('crud/_delete_form.html.twig') }}
{% endblock %}
